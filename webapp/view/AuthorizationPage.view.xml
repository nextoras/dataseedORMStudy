								justifyContent="Center"
								alignContent="Center">
								<items>
                            	<FormattedText htmlText="&lt;span style=font-size:46px>DataseedORM&lt;/span>"  visible="true" />
                            	</items>
                            </FlexBox>
							<FlexBox
								height="100%"
								width="100%"
								alignItems="Center"
								justifyContent="Center"
								alignContent="Center">
									<items>
										<Button text="{i18n>EnterBtn}" type="Emphasized" width="auto" iconFirst="true" enabled="true" visible="true" iconDensityAware="false" press="_onButtonPress"/>
									</items>
							</FlexBox>
						</layout:VerticalLayout>
					</items>
				</FlexBox>

        </content>
